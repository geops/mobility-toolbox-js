{
  "source": "./src/",
  "destination": "./apidoc",
  "includes": ["\\.js$"],
  "excludes": ["\\.test\\.js$", "doc/", "common", "(u|U)tils"],
  "plugins": [
    { "name": "esdoc-standard-plugin" },
    { "name": "esdoc-ecmascript-proposal-plugin", "option": { "all": true } },
    {
      "name": "esdoc-publish-html-plugin",
      "option": {
        "template": "./esdoc/template"
      }
    }
  ]
}
